// To test if statements in weird configurations

main:function integer() {
	i:integer = 0;
	while (i < 100) {
		if (i < 25) {
			i++;
		} else if (25 <= i && i < 50) {
			i = i+2;
		} else if (50 <= i && i < 75) {
			if (i % 2 == 0) return 500;
			i = i + 3;
		} else {
			i = i + 4;
		}
		print i;
	}
	return i;
}
